    },
    "content": {
      "type": "component",
      "repeatable": true,
      "component": "page.text-zone"
    },
    "url": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "members": {
      "type": "component",
      "repeatable": true,
      "component": "page.membre"
    },
    "soundcloudLink": {
      "type": "string"
    }
  }
}
